{
  "name": "laravel-mix-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for WordPress and Static site, based on Laravel Mix",
  "main": "index.js",
  "scripts": {
    "watch": "NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "npm run watch",
    "prod": "npm run production"
  },
  "keywords": [],
  "author": "dsktschy",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "browser-sync": "^2.26.5",
    "browser-sync-webpack-plugin": "^2.0.1",
    "eslint": "^5.14.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "imagemin": "^6.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^7.0.0",
    "laravel-mix": "^4.0.15",
    "laravel-mix-copy-watched": "^2.1.2",
    "laravel-mix-eslint": "^0.1.2",
    "laravel-mix-polyfill": "^1.0.1",
    "laravel-mix-pug": "git+https://github.com/dsktschy/laravel-mix-pug.git#develop-temporary",
    "laravel-mix-stylelint": "^0.2.0",
    "pug": "^2.0.3",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "stylelint": "^8.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^3.5.3",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-spritemap-webpack-plugin": "^3.2.1",
    "vue-template-compiler": "^2.5.21"
  },
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": ">=10.14.2",
    "npm": ">=6.4.1"
  }
}
